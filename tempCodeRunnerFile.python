import time


def prime(x): 
    for i in range(2, x):
        count = 0
        if x % i == 0:
            return False
        count = i
    print("count: ", count)
    return True

def prime2(x): 
    count = 0
    for i in range(2, int(x**0.5)):
        if x % i == 0:
            return False
        count = i
    print("count: ", count)
    return True

x = int(input())
start = time.time()
print(prime(x))
end = time.time()
print("time: ", end - start)